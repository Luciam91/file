{
    "name": "amphp/file",
    "homepage": "https://github.com/amphp/file",
    "description": "An async filesystem library for Amp.",
    "support": {
        "issues": "https://github.com/amphp/file/issues"
    },
    "keywords": [
        "file",
        "disk",
        "static",
        "async",
        "non-blocking",
        "amp",
        "amphp",
        "io",
        "filesystem"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net"
        },
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "amphp/amp": "^2",
        "amphp/byte-stream": "^1"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "amphp/phpunit-util": "^1",
        "amphp/parallel": "^1",
        "phpunit/phpunit": "^6",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "suggest": {
        "amphp/parallel": "Provides async file access without the eio or uv extension."
    },
    "autoload": {
        "psr-4": {
            "Amp\\File\\": "lib"
        },
        "files": ["lib/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\File\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev",
            "dev-amp_v2": "0.2.0-dev"
        }
    }
}
